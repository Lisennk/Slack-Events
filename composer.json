{
    "name": "lisennk/laravel-slack-events-api",
    "description": "Slack Events API for Laravel 5",
    "keywords": ["slack", "laravel", "api", "slack api", "slack events api", "events", "slack events"],
    "require": {
        "illuminate/http": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/routing": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7|^5.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Lisennk\\LaravelSlackEvents\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lisennk\\LaravelSlackEvents\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lisennk\\LaravelSlackEvents\\SlackEventsServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Lisennk",
            "email": "serewann@gmail.com"
        }
    ]
}
