{
    "name": "lisennk/laravel-slack-events-api",
    "description": "Slack Events API for Laravel 5, 6, 7 and 8",
    "keywords": ["slack", "laravel", "api", "slack api", "slack events api", "events", "slack events"],
    "require": {
        "illuminate/http": "^5.4||^6.2||^7.2||^8.0||^9.0",
        "illuminate/support": "^5.4||^6.2||^7.2||^8.0||^9.0",
        "illuminate/routing": "^5.4||^6.2||^7.2||^8.0||^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0||^9.0||^10.0",
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Lisennk\\LaravelSlackEvents\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lisennk\\LaravelSlackEvents\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lisennk\\LaravelSlackEvents\\SlackEventsServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Lisennk",
            "email": "serewann@gmail.com"
        }
    ]
}
